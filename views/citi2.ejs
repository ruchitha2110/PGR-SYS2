                                        <% if(item.whyRejectedByOff){ %>
                                        <p style="color: crimson;"><b>Why Rejected by Officer:&nbsp;&nbsp;</b>
                                            <%=item.whyRejectedByOff %></p>
                                        <form action="/deleteComplaint/<%= item._id %>/<%= user.username %> "
