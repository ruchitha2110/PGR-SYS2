                                <form action="/deleteComplaint/<%= item._id %>/<%= user.username %> " method="post">
                                    <button class="remove" type="submit">Remove</button>
                                </form>
                                <% }else{%>
                                    <button class="remove" style="color: green;">Assigned</button>
                                <% } %>
                            </li>
