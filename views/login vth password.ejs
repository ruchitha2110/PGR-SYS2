    <div class="div2">

      <% if(message.length > 0){ %>
      <div class="alert" style="width:665px;">
        <%= message %>
      </div>
      <% } %>


      <div class="tab">
        <button class="tablinks" onclick="openCity(event, 'Userlogin')" id="defaultOpen">USER LOGIN</button>
        <!-- <button class="tablinks" onclick="openCity(event, 'otplogin')">USER LOGIN USING OTP</button> -->

      </div>


      <div id="Userlogin" class="tabcontent">
        <!-- <form action="#" class="form"> -->
        <form class="form" action="/login" method="post">

          <h1 class="form_title"> Login Using Password </h1>
          <div class="form_div">

            <p>
              Username/Email*
            </p>
            <input type="text" name="username" class="form_input" placeholder="Username">
          </div>

          <div class="form_div">

            <p>
              Password*
            </p>
            <input type="password" name="password" class="form_input" placeholder="Password"><br>
          </div>
          <p style="font-size: 24px; color:#413437;font-weight:600;">
            Security Code
          </p>
          <div id="captcha" class="form_div">
            <input type="hidden" name="captcha" value="" class="input">
            <div class="preview"></div>
            <div class="captcha_form">
              <input type="text" id="captcha_form" name="inputCaptcha" class="form_input_captcha"
                placeholder="Security Code">

              <input type="button" class="captcha_refersh">
              <!-- <i class="fa fa-refresh"></i> -->
              </input>
            </div>

          </div>

          <input type="submit" class="form_button" value="LOGIN">
          <div class="link1">

            <ul>

              <li><a href="/forgotPass">Forgot password?</a></li>
              <li><a href="/forgotUsername">Forgot username?</a></li>

              <li><a href="/registration">Not a member? Register</a></li>
            </ul>
          </div>
        </form>
      </div>

      <div id="otplogin" class="tabcontent">
        <!-- <h4> LOGIN USING OTP</h4> -->
        <form class="form1" action="/login" method="post">

          <h1 class="form1_title"> Login Using Otp </h1>
          <div class="form1_div">

            <p>
              Username/Email Id/Mobile No
            </p>
            <input type="text" name="username" class="form1_input" placeholder="Username">
          </div>

          <div class="form1_div">

            <p>
              OTP
            </p>
            <div class="otp">
              <input type="OTP" name="OTP" placeholder="Password">
              <button type="button" onclick="generate()">Get OTP</button>
            </div>
          </div>
          <input type="submit" class="form_button" value="LOGIN">
          <div class="link2">

            <ul>

              <li><a href="/forgotPass">Forgot password?</a></li>
              <li><a href="/forgotUsername">Forgot username?</a></li>

              <li><a href="/registration">Not a member? Register</a></li>
            </ul>
          </div>

        </form>
      </div>
    </div>
